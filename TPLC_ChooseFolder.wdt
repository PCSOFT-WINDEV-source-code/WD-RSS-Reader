   -
     name : RefreshList
     procedure_id : 1760455005408234379
     type_code : 14
     code : |1+
      // Summary: Refreshes the list of folders
      // Syntax:
      //RefreshList ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE RefreshList()
      
      // Clears the list
      TableDeleteAll(TVT_Folders)
      
      // Adds the root
      TVT_Folders..ExpandedImage = IMG_RSSDirectory
      TVT_Folders..CollapsedImage = IMG_RSSDirectory
      TableAddLine(TVT_Folders, 0, "Subscriptions")
      
      
      // Defines the images for the folders
      TVT_Folders..ExpandedImage = IMG_Closed_Directory
      TVT_Folders..CollapsedImage = IMG_Closed_Directory
      
      // Fills the list
      FOR EACH Folder on Caption
      	TableAddChild(TVT_Folders, 1, Folder.FolderID, Folder.Caption)
      END
     type : 458752
