   -
     code : |1+
      PROCEDURE MyWindow(gnPostID is 8-byte int)
      
      // Default return value
      MyWindow..ReturnedValue = False
      
      // Reads the post
      HReadSeekFirst(Post, PostID, gnPostID)
      IF HFound(Post) = False THEN
      	Error(<§$0000§>)
      	Close()
      END
      
      // Title of the window
      MyWindow..Title = <§$0001§>+Post.Title
      
      // Text
      STC_Explanations = StringBuild(STC_Explanations, Post.Title)
   -
