  procedures :
   -
     name : FillList
     procedure_id : 1760454940968964027
     type_code : 14
     code : |1-
      // Summary: Fills the list
      // Syntax:
      // FillList ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE FillList()
      
      // Clears the list
      LooperDeleteAll(LOOP_Tags)
      
      // Browses the existing tags
      FOR EACH Tag 
      	// Adds the tag
      	let nSubscript = LooperAddLine(LOOP_Tags, Tag.TagID, Tag.Caption)
      	// If an association exists
      	HSeekFirst(Association,AssociationID,[gnPostID, Tag.TagID])
      	IF HFound(Association) THEN
      		// Displays the tag in bold and checks the line
      		ATT_Associated[nSubscript] = True
      	END
      END
      
      // Browses the suggestions
      FOR EACH string sASuggestion of Post.Category SEPARATED BY ";"
      	
      	// If the suggestion is not already found
      	IF LooperSeek(ATT_Caption, sASuggestion) <= 0 THEN
      		// Adds the suggestion
      		LooperAddLine(LOOP_Tags, 0, sASuggestion)
      	END
      		
      END
      
      // Sort
      LooperSort("ATT_Caption")
