      -
        code : |1+
         // Deletes the current associations
         FOR EACH Association WHERE PostID = gnPostID
         	HDelete(Association)
         END
         
         // Stores the associations
         FOR ALL ROW nSubscript of LOOP_Tags
         	// If the current tag is flagged as associated
         	IF ATT_Associated[nSubscript] THEN
         		// If the tag does not exist yet
         		IF ATT_TagID[nSubscript] <= 0 THEN
         			// Saves the tag
         			HReset(Tag)
         			Tag.Caption = ATT_Caption[nSubscript]
         			HAdd(Tag)
         			ATT_TagID[nSubscript] = Tag.TagID
         		END
         		// Saves the association
         		HReset(Association)
         		Association.PostID = gnPostID
         		Association.TagID =ATT_TagID[nSubscript] 
         		HAdd(Association)
         		
         	END
         END
         
         
         // Closes the window in validation
         Close("", True)
