     procedure_id : 1760454949559228627
     type_code : 14
     code : |1-
      // Summary: Checks/Unchecks node
      // Syntax:
      //CheckNodes ([<bChecked> is boolean])
      //
      // Parameters:
      //	bChecked (boolean - default value=1): Value of the checkmark
      // Return value:
      // 	None
      //
      PROCEDURE CheckNodes(LOCAL bCheckmark is boolean = True)
      
      
      // Browses the folders
      sFolder is string = TreeGiveChild(TREE_Export, "", tvFirst)
      WHILE sFolder <> ""
      	
      	// Checks the folder
      	TREE_Export[sFolder]..Checked = bCheckmark
      	
      	// Browses the streams
      	sStream is string = TreeGiveChild(TREE_Export, sFolder, tvFirst)
      	WHILE sStream <> ""
      		
      		// Checks the stream
      		TREE_Export[sFolder+TAB+sStream]..Checked = bCheckmark
      	
      		// Next stream	
      		sStream = TreeGiveChild(TREE_Export, sFolder, tvNext)
      		
      	END
      	
      	// Next folder
      	sFolder = TreeGiveChild(TREE_Export, "", tvNext)
      	
      END
      	
     type : 458752
   -
     name : FillFolder
     internal_properties : CAAAAAgAAACRVLQlEEJMvb26/b84IefJwHstkSezLweBE3XoG/UeSERCoTwsuUxChRp0aS49Qtj6ujsH6oQ2XZA/QlpQ3EqLZiS3tsEP2TWuHjzSG3InDm8ClK7LF/3ElmRAALHXtw4Tp4KOdJjNZPa2hhNwE0TNnUhQ9fSUivdW7qzMq+jup0lUGOisXKzrTavsnouQe9Y5rk+ILlcURwPaQtRA6r69pGA=
     procedure_id : 1760454949559294163
     type_code : 14
     code : |1+
      // Summary: Fills the folders
      // Syntax:
      //FillFolder ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE FillFolder()
      
      // Clears the treeview
      TreeDeleteAll(TREE_Export)
      
      // Subscriptions
      TreeAdd(TREE_Export, <§$0005§>, IMG_RSSDirectory, IMG_RSSDirectory, 0)
      TREE_Export[<§$0005§>]..Checked = True 
      
      // Adds the folders
      FOR EACH Folder on Caption
      	TreeAdd(TREE_Export, <§$0005§> +TAB+ Folder.Caption, IMG_Closed_Directory, IMG_Closed_Directory, Folder.FolderID)	
      	// Checks the node
      	TREE_Export[<§$0005§> +TAB+ Folder.Caption]..Checked = True 
      END
      
      // Expands the TreeView
      TreeExpandAll(TREE_Export)
      
     type : 458752
