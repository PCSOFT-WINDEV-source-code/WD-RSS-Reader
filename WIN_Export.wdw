    name : BTN_Export
    identifier : 0x286e6473517a58d3
    internal_properties : 
    properties :
     x : 432
     y : 385
     width : 100
     height : 30
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : 
        code : |1+
         bAtLeastOneCheckmark 	is boolean = False
         nFolderSubscript		is int
         nStreamSubscript			is int
         sExportedList 		is string
         sExport 			is string
         
         sModel is string = [
         <opml version="1.1">
         <head>
         <title>%1</title>
         <dateModified>%2</dateModified>
         </head>
         <body>
         %3
         </body>
         </opml>
         ]
         
         // If the "Subscriptions" folder is checked
         IF TREE_Export[<§$0005§>]..Checked THEN
         	// Exports the streams without folder
         	FOR EACH RSSStream WHERE FolderID = 0
         		bAtLeastOneCheckmark = True
         		sExportedList += [CR] + StringBuild("<outline text=""%1"" xmlUrl=""%2""/>", Replace(StringToUTF8(RSSStream.Title), "&", "amp;"), RSSStream.Address)
         	END
         END
         
         // Browses the sub-folders
         sFolder is string = TreeGiveChild(TREE_Export, <§$0005§>, tvFirst)
         WHILE sFolder <> ""
         	
         	// If the folder is checked
         	IF TREE_Export[<§$0005§> +TAB+ sFolder]..Checked THEN
         		
         		// Stores the identifier in the list
         		IF TreeIdentifier(TREE_Export, <§$0005§> +TAB+ sFolder) > 0 THEN
         			// At least one folder is checked
         			bAtLeastOneCheckmark = True
         			nFolderSubscript++
         			
         			// Reads the folder
         			HReadSeekFirst(Folder, FolderID, TreeIdentifier(TREE_Export, <§$0005§> +TAB+ sFolder))
         			IF HFound(Folder) = False THEN CONTINUE
         			
         			// Folder caption
         			sExportedList += [CR] + StringBuild("<outline text=""%1"">", Replace(StringToUTF8(Folder.Caption), "&", "amp;"))
         									
         			// Browses the streams
         			nStreamSubscript = 1
         			FOR EACH RSSStream WHERE FolderID = Folder.FolderID
         				sExportedList += [CR] + StringBuild("<outline text=""%1"" xmlUrl=""%2""/>", Replace(StringToUTF8(RSSStream.Title), "&", "amp;"), RSSStream.Address)
         			END
         			sExportedList += [CR] + "</outline>"
         		END
         		
         	END
         	
         	// Next folder
         	sFolder = TreeGiveChild(TREE_Export, <§$0005§>, tvNext)
         	
         END
         
         // If no folder is checked
         IF bAtLeastOneCheckmark = False THEN
         	Error(<§$0003§>)
         	ReturnToCapture(TREE_Export)
         END
         
         // Builds the export
         sExport = StringBuild(sModel, <§$0004§>, DateToString(Today()+Now(),maskEmailDate), sExportedList)
         
         // Opens the file picker
         sFile is string
         sFile = fSelect("", "", <§$0001§>, <§$0002§> + TAB + "*.opml", "*.opml", fselCreate + fselExist)
         // If a file is selected
         IF sFile ~= "" THEN RETURN
         
         // Stores the buffer in the XML file
         fSaveText(sFile, sExport)
         
         // Opens the directory in the Windows explorer
         ShellExecute(fExtractPath(sFile, fDrive+fDirectory))
         
         // Message for end of process
         ToastDisplay(<§$0000§>, toastShort, vaMiddle, haCenter)
         
         // Closes the window
         Close()
         
