     name : DnD_BeginDrag
     procedure_id : 1760454988220300330
     type_code : 14
     code : |1+
      // Summary: Procedure called at the beginning of drag in DnD mode
      // Syntax:
      // DnD_BeginDrag ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE DnD_BeginDrag()
      
      // By default, no stream is moving
      gnMovedStreamID = -1
      
      // Retrieves the subscript of the row
      nRowSubscript is int = TableInfoXY(TVT_Stream, tiLineNumber, MouseXPos(), MouseYPos())
      // The subscript must be valid
      IF nRowSubscript <= 0 THEN
      	DnDCursor(dndNone)
      	RETURN
      END
      
      // The line must correspond to a stream
      IF IsNumeric(TVT_Stream.COL_ID[nRowSubscript]) = False THEN
      	DnDCursor(dndNone)
      	RETURN
      END
      
      // Allows the move
      DnDCursor(dndMove)
      
      // Stores the identifier of the stream
      gnMovedStreamID = TVT_Stream.COL_ID[nRowSubscript]
