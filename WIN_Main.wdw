      // Summary: Customizes the menu
      // Syntax:
      //Tags_CreateMenuOptions ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE Tags_CreateMenuOptions()
      
      nSubscript is int
      
      
      // Delete the previous tags
      nSubscript = 1
      sOption is string =  OPTIONTAGS+nSubscript
      WHILE MenuExist(MENU_Post_Tag, sOption)
      	// Deletes the option
      	MenuDelete(sOption)
      	// Next option
      	nSubscript++
      	sOption =  OPTIONTAGS+(nSubscript)
      END
      
      // Adds all the stored tags
      nSubscript = 1
      FOR EACH Tag on Caption
      	
      	// Adds the tag
      	MenuAddOption(MENU_Post_Tag,  OPTIONTAGS+nSubscript, Tag.Caption, Tags_Check, Tag.TagID)
      	nSubscript++
      	
      END
