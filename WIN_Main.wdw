      // Summary: Fills the treeview
      // Syntax:
      //FillTreeView ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE FillTreeView()
      
      nSubscript 			is int
      nFolderSubscript	 	is int
      
      
      // Clears the list
      TableDeleteAll(TVT_Stream)
      
      // Standby
      FillTreeView_Standby()
      
      // Subscriptions
      TVT_Stream..ExpandedImage = IMG_RSSDirectory
      TVT_Stream..CollapsedImage = IMG_RSSDirectory
      nSubscript = TableAddLine(TVT_Stream, SUBSCRIPTIONID, <§$0003§>, 0, "2", <§$0003§>)
      FOR EACH Folder on Caption
      	
      	// Adds the folder
      	nFolderSubscript = FillTreeView_Folder(Folder.FolderID)
      		
      	// Browses the folder streams
      	FOR EACH RSSStream on Title WHERE "FolderID = "+Folder.FolderID
      		// Adds the stream
      		FillTreeView_Stream(RSSStream.RSSStreamID)		
      	END
      	
      	// Will the node be collapsed or expanded?
      	IF OptionRead(OPTION_GUI_EXPANDEDNODES) [~] PREFIX_FOLDER+Folder.FolderID THEN
      		TableExpand(TVT_Stream, nFolderSubscript)
      	ELSE
      		TableCollapse(TVT_Stream, nFolderSubscript)
      	END
      	
      END
      
      // Browses the streams without folder
      FOR EACH RSSStream on Title WHERE "FolderID = 0"
      	// Finds the stream in the list
      	FillTreeView_Stream(RSSStream.RSSStreamID)
      END
      
      // Expands the "Subscriptions" branch
      TableExpand(TVT_Stream, nSubscript)
      
      // Tags
      FillTreeView_Tags()
      
      // Selects the node that was selected during the previous closing
      sNode is string = OptionRead(OPTION_GUI_CURRENTNODE)
      // If no node was selection during the closing (1st start for example)
      IF sNode = "" THEN
      	// Retrieves the subscript of the root subscription row
      	nSubscript = TableSeek(COL_ID, SUBSCRIPTIONID)
      ELSE
      	// Retrieves the subscript of the row
      	nSubscript = TableSeek(COL_ID, sNode)
      	// Retrieves the subscript of the root subscription row if necessary
      	IF nSubscript <= 0 THEN nSubscript = TableSeek(COL_ID, SUBSCRIPTIONID)
      END
      // Selects the row
      TableSelectPlus(TVT_Stream, nSubscript)
      // Applies the display
      ExecuteProcess(TVT_Stream, trtSelection)
      
      // Initializes the counters
      CountInitialize()
      // Applies the count on the GUI
      CountApply()
