     procedure_id : 1760454988218858538
     type_code : 14
     code : |1-
      // Summary: Displays the post
      // Syntax:
      //Post_OpenInBrowser (<nPostID> is 8-byte int)
      //
      // Parameters:
      //	nPostID (8-byte integer): Identifier of the post
      // Return value:
      // 	None
      //
      PROCEDURE Post_OpenInBrowser(LOCAL nPostID is 8-byte int)
      
      nPostPosition is int
      nStreamPosition 	is int
      sLink 			is string
      
      nPostPosition = HSavePosition(Post, hSaveItems)
      nStreamPosition = HSavePosition(RSSStream, hSaveItems)
      
      // Finds the post
      HReadSeekFirst(Post, PostID, nPostID)
      IF HFound(Post) THEN
      	
      	// Starts the default browser on the post
      	IF Post.Link <> "" THEN
      		sLink = Post.Link
      	ELSE
      		HReadSeekFirst(RSSStream, RSSStreamID, Post.RSSStreamID)
      		IF HFound(RSSStream) THEN
      			// Is a link defined on the stream?
      			IF RSSStream.Link <> "" THEN
      				sLink = Post.Link
      			ELSE
      				// At last, stores the link of the stream
      				sLink = RSSStream.Address
      			END
      		END
      	END
      END
      
      // If a link was found
      IF sLink <> "" THEN
      	ShellExecute(Post.Link)
      ELSE
      	// If there is no link, inform the user
      	ToastDeleteAll()
      	ToastDisplay(<§$0005§> +CR+ <§$0006§>, toastShort, vaMiddle, haCenter)
      END
      
      
      END :
      HRestorePosition(nPostPosition)
      HRestorePosition(nStreamPosition)
