     procedure_id : 1760454988218137642
     type_code : 14
     code : |1+
      // Summary: Checks whether it is the first start of the application and proposes suitable actions
      // Syntax:
      // DetectFirstStart ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE DetectFirstStart()
      
      // If the database contains no stream
      IF HNbRec(RSSStream) = 0 THEN
      	
      	// Opens the window for first start
      	sImportwindow is string = Open(WIN_FirstStartup)
      	
      	// If the selected option requires the opening of a window (importing an OPML file for example)
      	SWITCH sImportwindow
      		// No return
      		CASE ""
      			// No action
      		
      		// default (default imported streams)
      		CASE "default"
      			// Refreshes the display
      			FillTreeView()
      		
      		// other cases: window for importing or adding streams
      		CASE "WIN_Import"
      		
      		// Opens the requested window
      		arrStreamID is array of 8-byte int = Open(WIN_Import)
      		IF arrStreamID..Count > 0 THEN
      			// Refreshes the display
      			FillTreeView()			
      		END
      		
      		CASE "WIN_NewStream"
      			// Opens the requested window
      			nStreamID is 8-byte int = Open(WIN_NewStream)
      			IF nStreamID > 0 THEN
      				// Refreshes the display
      				FillTreeView()				
      
      			END
      			
      	END
      	
      END
