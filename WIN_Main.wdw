     name : FillTreeView_Stream
     internal_properties : CAAAAAgAAAC/+a7lEY1/ojCbAJxBtWJJkkdvnC/YTJk3VUZ03OHJoefKCDz95kUddoXj+gg26wTtGOjoBh7rD+taQXIDzalJgISdyKQFne9NV4MZYbeD72Dzwbp5zas/vjcG8vGphefmEf7ryL76HtDiA1g=
     procedure_id : 1760454988218989610
     type_code : 14
     code : |1-
      // Summary: Adds a stream into the treeview
      // Syntax:
      //[ <Result> = ] FillTreeView_Stream (<nRSSStreamID> is 8-byte int)
      //
      // Parameters:
      //	nRSSStreamID (8-byte integer): Identifier of the stream
      // Return value:
      // 	integer: Subscript of the stream
      //
      PROCEDURE FillTreeView_Stream(nRSSStreamID is 8-byte int) : int
      
      nStreamSubscript is int
      sStreamImage is string
      nFolderSubscript is int
      nPosition is int = -1
      
      
      nPosition = HSavePosition(RSSStream, hSaveItems)
      
      // Reads the record
      HReadSeekFirst(RSSStream, RSSStreamID, nRSSStreamID)
      IF HFound(RSSStream) = False THEN RESULT -1
      
      // Checks whether the stream is already found
      nStreamSubscript = TableSeek(TVT_Stream.COL_ID, nRSSStreamID)
      IF nStreamSubscript <= 0 THEN
      	
      	// Retrieves the subscript of the folder
      	IF RSSStream.FolderID >  0 THEN
      		nFolderSubscript = FillTreeView_Folder(RSSStream.FolderID)
      	ELSE
      		nFolderSubscript = TableSeek(TVT_Stream.COL_ID, SUBSCRIPTIONID)
      	END
      	
      	// The subscript of the parent folder must be valid
      	IF nFolderSubscript <= 0 THEN RESULT -1
      	
      	// Adds the stream
      	nStreamSubscript = TableAddChild(TVT_Stream, nFolderSubscript, RSSStream.RSSStreamID, RSSStream.Title, 0)
      		
      END
      
      // Updates the stream elements that can change
      TVT_Stream[nStreamSubscript].COL_InitialCaption = RSSStream.Title
      TVT_Stream[nStreamSubscript].COL_Sort = "1"+RSSStream.Title
      
      // Defines the image
      sStreamImage = RSSStream_GetCacheImage(nRSSStreamID)	
      TVT_Stream[nStreamSubscript]..ExpandedImage = sStreamImage
      TVT_Stream[nStreamSubscript]..CollapsedImage = sStreamImage
      
      // Sorts the parent folder
      TableSortChild(TVT_Stream, nFolderSubscript, "COL_Sort")
      
      // Retrieves the subscript of the stream (may have been modified by the sort)
      nStreamSubscript = TableSeek(TVT_Stream.COL_ID, nRSSStreamID)
      
      // Updates the stream counter
      //UPD_Counter_RSSStream(nRSSStreamID)
      
      // Returns the subscript of the stream
      RESULT nStreamSubscript
      
      END :
      IF nPosition <> -1 THEN HRestorePosition(nPosition)
