      // Summary: Changes the systray picto according to the number of unread messages
      // Syntax:
      //Systray_ChangePicto ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE Systray_ChangePicto()
      
      nNbUnreadPosts 	is int
      sIcon 				is string
      sMessage 			is string
      
      // Configures the query (all the unread posts must be counted)
      QRY_NbUnreadPosts.pStart 		= Null
      QRY_NbUnreadPosts.pEnd 		= Null
      QRY_NbUnreadPosts.pIDFolder 	= Null
      QRY_NbUnreadPosts.pStreamID 	= Null
      
      // Runs the query
      IF HExecuteQuery(QRY_NbUnreadPosts) = False THEN RETURN
      
      // Reads the result
      HReadFirst(QRY_NbUnreadPosts)
      IF HOut(QRY_NbUnreadPosts) = False THEN
      	// Stores the number of unread posts
      	nNbUnreadPosts = QRY_NbUnreadPosts.NbPosts
      END
      
      // Defines the icon and the message according to the number of unread posts
      SWITCH nNbUnreadPosts
      	// All the posts are read
      	CASE 0
      		sIcon = ".\rss.ico"
      		sMessage = <§$0002§> +CR+ <§$0029§>
      	
      	// 1 post is not read
      	CASE 1
      		sIcon = ".\rss-new.ico"
      		sMessage = <§$0002§> +CR+ <§$002a§>
      	
      	// At least 2 posts are not read
      	OTHER CASE
      		sIcon = ".\rss-new.ico"
      		sMessage = <§$0002§> +CR+ nNbUnreadPosts+<§$002b§>
      END
      
      
      // Modifies the icon
      SysIconModify(sIcon, sMessage)
