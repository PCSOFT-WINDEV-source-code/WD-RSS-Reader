     name : RSSStream_Display
     procedure_id : 1760454988218268714
     type_code : 14
     code : |1+
      // Summary: Displays an RSS stream in the list
      // Syntax:
      //RSSStream_Display (<nRSSStreamID> is 8-byte int)
      //
      // Parameters:
      //	nRSSStreamID (8-byte integer): Identifier of the RSS stream
      // Return value:
      // 	None
      //
      PROCEDURE RSSStream_Display(LOCAL nRSSStreamID is 8-byte int)
      
      nFolderSubscript is int
      nRootSubscript is int
      sStreamImage is string
      nStreamSubscript is int
      
      // Reads the RSS stream
      HReadSeekFirst(RSSStream, RSSStreamID, nRSSStreamID)
      IF HFound(RSSStream) = False THEN RETURN
      
      // Stores the subscript of the root (Subscription)
      nRootSubscript = TableSeek(TVT_Stream.COL_ID, SUBSCRIPTIONID)
      
      // Finds the folder of the stream
      IF RSSStream.FolderID = 0 THEN
      	// If no folder, the stream is positioned at the root
      	nFolderSubscript = nRootSubscript
      ELSE
      	
      	// Adds the folder
      	nFolderSubscript = FillTreeView_Folder(RSSStream.FolderID)
      	
      END
      
      // The subscript of the parent folder must be valid
      IF nFolderSubscript <= 0 THEN RETURN
      
      // Retrieves the image of the stream
      sStreamImage = RSSStream_GetCacheImage(RSSStream.RSSStreamID)
      
      // Stores the current subscript of the stream
      nStreamSubscript = FillTreeView_Stream(RSSStream.RSSStreamID)
      
      // Modifies the caption and the image
      TVT_Stream[nStreamSubscript].COL_Caption = RSSStream.Title
      TVT_Stream[nStreamSubscript].COL_InitialCaption = RSSStream.Title
      TVT_Stream[nStreamSubscript].COL_Sort = "1"+RSSStream.Title
      TVT_Stream[nStreamSubscript]..ExpandedImage = sStreamImage
      TVT_Stream[nStreamSubscript]..CollapsedImage = sStreamImage
      
      // Sorts the table
      TableSortChild(TVT_Stream, nFolderSubscript, "COL_Sort")
