     name : CheckStream
     procedure_id : 1760454988220562474
     type_code : 14
     code : |1+
      // Summary: Checks the validity of stored streams
      // Syntax:
      // CheckStream ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE CheckStream()
      
      arrCheckTasks is array of ParallelTask
      nStreamID is 8-byte int
      bReturn is boolean
      sReturn is string
      arrInErrorID is array of 8-byte int
      sListErrors is string
      
      // A check task must be started for each stream
      FOR EACH RSSStream 
      	
      	// Stores the task in an array
      	Add(arrCheckTasks, ParallelTaskExecute(TP_rssCheckStream, (RSSStream.RSSStreamID)))
      	
      END
      
      // Waits for the end of tasks
      ParallelTaskWaitAll(arrCheckTasks)
      
      // Browses the tasks
      FOR ALL ELEMENT MyTask of arrCheckTasks
      	
      	// Retrieves the return values
      	(nStreamID, bReturn, sReturn) = MyTask.ValeurRenvoyée
      	// If the stream is in error
      	IF bReturn = False THEN
      		HReadSeekFirst(RSSStream, RSSStreamID, nStreamID)
      		IF HFound(RSSStream) THEN
      			sListErrors += [CR+CR] + StringBuild(" - %1 (%2)"+CR+"%3", RSSStream.Title, RSSStream.Address, sReturn)
      			// Stores the stream to delete it if necessary
      			Add(arrInErrorID, nStreamID)
      		END
      	END
      	
      END
      
      // If the array is empty, no action
      IF arrInErrorID..Count = 0 THEN RETURN
      
      // Copies the list of errors into the clipboard
      ToClipboard(sListErrors)
      
      // Displays the streams in error
      IF Dialog(<§$0068§>+CR+CR+sListErrors+CR+CR+<§$0069§>+CR+CR+<§$006a§>, [<§$006b§>, <§$006c§>], 1, 2, dlgIconQuestion) = 1 THEN
      	// Deletes the streams in error
      	FOR ALL ELEMENT nStreamID of arrInErrorID
      		ParallelTaskExecute(TP_rssDeleteStream, (nStreamID))
      	END
      END
