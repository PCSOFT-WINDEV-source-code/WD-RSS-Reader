      // Summary: Displays the posts of the current selection (in case of UPD)
      // Syntax:
      //DisplaySelectionPosts_UPD ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE DisplaySelectionPosts_UPD()
      
      WIN_Main..DisplayEnabled = False
      
      arrUpdatedPosts is array of STPost
      
      // Reloads the current posts
      QRY_ListPosts.pListPostID = ""
      FOR ALL ELEMENT stAPost of garrPosts
      	QRY_ListPosts.pListPostID += [";"] + stAPost.nPostID
      END
      // Runs the query
      HExecuteQuery(QRY_ListPosts)
      // Loads the results in the array
      FileToArray(garrPosts, QRY_ListPosts)
      
      
      // No parameter by default
      QRY_ListPosts.pFavorites = Null
      QRY_ListPosts.pIDFolder = Null
      QRY_ListPosts.pRSSStreamID = Null
      QRY_ListPosts.pIDTag = Null
      QRY_ListPosts.pDay = Null
      QRY_ListPosts.pLu = Null
      QRY_ListPosts.pListPostID = Null
      
      // The menu to open depends on the click position
      SWITCH COL_ID[TVT_Stream]
      	// It's a folder
      	CASE [= PREFIX_FOLDER
      		// Displays all the posts of the folder streams
      		QRY_ListPosts.pIDFolder = ExtractString(TVT_Stream.COL_ID[TVT_Stream], 2,"_")
      			
      	// It's the root
      	CASE SUBSCRIPTIONID
      		
      	// "Standby" folder
      	CASE STANDBYID
      		
      	// "Not read" folder
      	CASE NOTREADID
      		QRY_ListPosts.pLu = False
      	
      	// "Favorites" folder
      	CASE FAVORITESID
      		QRY_ListPosts.pFavorites = True
      	
      	// "Today" folder
      	CASE TODAYID
      		QRY_ListPosts.pDay = Today()
      	
      	// "Yesterday" folder
      	CASE YESTERDAYID
      		QRY_ListPosts.pDay = Yesterday()
      		
      	// Tags		
      	CASE [= PREFIX_TAG
      		// Displays all the posts flagged by the tag
      		QRY_ListPosts.pIDTag = ExtractString(TVT_Stream.COL_ID[TVT_Stream], 2,"_")
      	
      	// "Tags" folder
      	CASE TAGSID
      
      	// It's a stream
      	OTHER CASE
      		// Displays all the posts of the stream
      		QRY_ListPosts.pRSSStreamID = TVT_Stream.COL_ID[TVT_Stream]
      
      END
      
      // Runs the query
      HExecuteQuery(QRY_ListPosts)
      
      // Loads the results in an array
      FileToArray(arrUpdatedPosts, QRY_ListPosts)
      
      // Adds the new posts
      FOR ALL ELEMENT stANewPost of arrUpdatedPosts
      	IF Seek(garrPosts, asLinear, "nPostID", stANewPost.nPostID) <= 0 THEN
      		Add(garrPosts, stANewPost)
      	END
      END
      
      // Sorts the results by date, title
      ArraySort(garrPosts,asMember,"-dtDPublication;+sTitle")
      
      // Stores the selections
      nTablePostID is 8-byte int
      nSubscript is int = TableSelect(TABLE_Posts)
      IF nSubscript > 0 THEN nTablePostID = TABLE_Posts.COL_PostID[nSubscript]
      
      // Refreshes the table
      TableDisplay(TABLE_Posts, taInit)
      // Refreshes the looper
      LooperDisplay(LOOP_Posts, taInit)
      
      // Clears the preview
      HTM_Preview = ""
      
      // Repositions the selections
      IF nTablePostID > 0 THEN
      	nSubscript = TableSeek(TABLE_Posts.COL_PostID, nTablePostID)
      	IF nSubscript > 0 THEN TableSelectPlus(TABLE_Posts, nSubscript) ; ExecuteProcess(TABLE_Posts, trtSelection)
      END
      
      // If no post is listed
      IF garrPosts..Count = 0 THEN
      	// Displays the plane 3
      	WIN_Main..Plane = 3
      ELSE
      	// Repositions the plane if necessary
      	IF WIN_Main..Plane = 3 THEN WIN_Main..Plane = OptionRead(OPTION_GUI_MODE)
      END
      
      END:
      WIN_Main..DisplayEnabled = True
     type : 458752
