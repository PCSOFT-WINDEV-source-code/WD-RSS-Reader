        code : |1+
         PROCEDURE CollapsedExpanded()
         
         // if the automatic management of collapse is not enabled, no action
         IF Val(OptionRead(OPTION_GUI_EXPANDFOLDERAUTO)) = False THEN RETURN
         
         // Retrieveds the subscript that was collapsed/expanded
         nSubscript is int = MySelf
         
         // If the node is not expanded (it was collapsed), no action
         IF TableItemStatus(MySelf, nSubscript) <> taCollapse THEN RETURN
         
         nChildSubscript is int
         // Stores the subscript of the "Subscriptions" node
         nSubscriptionSubscript is int = TableSeek(COL_ID, SUBSCRIPTIONID)
         // Browses the direct children of the "Subscription" node
         nChildSubscript = TableGiveChild(MySelf, nSubscriptionSubscript, taFirst)
         WHILE nChildSubscript <> -1
         	// It is not the node that was just expanded
         	IF nChildSubscript <> nSubscript THEN
         		// Collapses the node)
         		TableCollapse(MySelf, nChildSubscript)
         	END
         	nChildSubscript = TableGiveChild(MySelf, nSubscriptionSubscript, taNext)
         END
        type : 36
