      // Summary: Applies the count to the treeview
      // Syntax:
      //CountApply ([<nStreamID> is 8-byte int [, <bUPDSelection> is boolean]])
      //
      // Parameters:
      //	nStreamID (8-byte integer - default value=0): Identifier of the stream to update
      //	bUPDSelection (boolean - default value=1): True for adding posts, False for reading posts
      // Return value:
      // 	None
      //
      PROCEDURE CountApply(LOCAL nStreamID is 8-byte int = 0, LOCAL bUPDSelection is boolean = True)
      
      // If only the stream counter (and its "parents") must be updated
      IF nStreamID > 0 THEN
      	
      	nIDFolder is 8-byte int
      	
      	// Retrieves the folder identifier
      	nPosition is int = HSavePosition(RSSStream, hSaveItems)
      	HReadSeekFirst(RSSStream, RSSStreamID, nStreamID)
      	IF HFound(RSSStream) THEN nIDFolder = RSSStream.FolderID
      	HRestorePosition(nPosition)
      	
      	// Applies the count for:
      	// - the stream
      	CountApply_AnIdentifier(nStreamID, bUPDSelection)
      	// - its folder
      	IF RSSStream.FolderID > 0 THEN CountApply_AnIdentifier(PREFIX_FOLDER+nIDFolder, bUPDSelection)
      	// - the root
      	CountApply_AnIdentifier(SUBSCRIPTIONID, bUPDSelection)
      	// - the standby
      	CountApply_AnIdentifier(NOTREADID, bUPDSelection)
      	CountApply_AnIdentifier(FAVORITESID, bUPDSelection)
      	CountApply_AnIdentifier(TODAYID, bUPDSelection)
      	CountApply_AnIdentifier(YESTERDAYID, bUPDSelection)
      	
      ELSE // If all the counters must be updated
      	
      	// Browses the count array
      	FOR ALL ELEMENT nNbPosts, sIdentifier of SET_Global.gaaCount
      		// Applies the count for each identifier
      		CountApply_AnIdentifier(sIdentifier, bUPDSelection)	
      	END
      	
      END
      
      // Adapts the systray icon
      Systray_ChangePicto()
     type : 458752
