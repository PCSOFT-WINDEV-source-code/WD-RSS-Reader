      // Summary: Adds a tag for a post
      // Syntax:
      //Tags_Add (<sTag> is string)
      //
      // Parameters:
      //	sTag (ANSI string): Tag to add
      // Return value:
      // 	None
      //
      PROCEDURE Tags_Add(LOCAL sTag is string)
      
      // Stores the identifier of the post
      nPostID is 8-byte int = gnCurrentPostID
      IF gnCurrentPostID <= 0 THEN RETURN
      
      // Finds the tag
      HReadSeekFirst(Tag, Caption, sTag)
      IF HFound(Tag) = False THEN
      	HReset(Tag)
      	Tag.Caption = sTag
      	HAdd(Tag)
      END
      
      // Adds or deletes the association
      HReadSeekFirst(Association, AssociationID, [nPostID, Tag.TagID])
      IF HFound(Association) THEN
      	HDelete(Association)
      ELSE
      	HReset(Association)
      	Association.PostID 	= nPostID
      	Association.TagID 		= Tag.TagID
      	HAdd(Association)
      END
      
      // Forces the treeview to be filled (to add the new tag)
      FillTreeView_Tags()
      // Re-forces the creation of the initial menu (an additional tag)
      Tags_CreateMenuOptions()
      
      // Updates the counter for this tag
      UPD_Counter_Tag(Tag.TagID)
      CountApply_AnIdentifier(PREFIX_TAG+Tag.TagID, False)
      
      
      // Refreshes the display of the row
      nSubscript is int
      sAssociatedTags is string = Post_ListAssociatedTags(gnCurrentPostID)
      
      nSubscript = TableSeek(COL_PostID, gnCurrentPostID)
      IF nSubscript > 0 THEN COL_AssociatedTags[nSubscript] = sAssociatedTags
      
      nSubscript = LooperSeek(ATT_PostID, gnCurrentPostID)
      IF nSubscript > 0 THEN ATT_AssociatedTags[nSubscript] = sAssociatedTags
