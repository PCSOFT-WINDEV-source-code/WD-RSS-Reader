      // Summary: Changes the display mode
      // Syntax:
      //ChangeDisplayMode (<nNewMode> is int)
      //
      // Parameters:
      //	nNewMode (integer): Display mode to use (corresponds to the plane)
      // Return value:
      // 	None
      //
      PROCEDURE ChangeDisplayMode(LOCAL nNewMode is int)
      
      // Forces the pressed status of the new mode to True
      {"BTN_Display_"+nNewMode, indControl}..Pushed = True
      
      // Retrieves the current display mode
      nCurrentMode is int = OptionRead(OPTION_GUI_MODE)
      
      // If the current display mode is the requested one, no action
      IF nNewMode = nCurrentMode THEN RETURN
      
      // Modifies the pressed status of the current "display mode" button
      {"BTN_Display_"+nCurrentMode, indControl}..Pushed = False
      
      // Stores the display mode
      OptionWrite(OPTION_GUI_MODE, nNewMode)
      
      // If the application is positioned on the "no content" plane, don't change
      IF MyWindow..Plane = 3 THEN RETURN
      
      // Changes the display with effect
      AnimationPrepare(MyWindow)
      MyWindow..Plane = nNewMode
      AnimationPlay(animFadeIn)
