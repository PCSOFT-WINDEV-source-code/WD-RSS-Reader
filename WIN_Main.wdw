      // Summary: Procedure called during the rollover in DnD mode
      // Syntax:
      //DnD_Rollover ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE DnD_Rollover()
      
      // Retrieves the subscript of the row
      nRowSubscript is int = TableInfoXY(TVT_Stream, tiLineNumber, MouseXPos(), MouseYPos())
      // The subscript must be valid
      IF nRowSubscript <= 0 THEN
      	DnDCursor(dndNone)
      	RETURN
      END
      
      // The dnd must be enabled (on a stream
      IF gnMovedStreamID <= 0 THEN
      	DnDCursor(dndNone)
      	RETURN
      END
      
      // Allows or not the DnD according to the corresponding identifier
      SWITCH TVT_Stream.COL_ID[nRowSubscript]
      
      	// "Standby" folder or tags: dnd not alllowed
      	CASE STANDBYID, NOTREADID, FAVORITESID, TODAYID, YESTERDAYID, TAGSID, [= PREFIX_TAG
      		DnDCursor(dndNone)
      	
      	// Other cases: root folder, folder: dnd allowed
      	OTHER CASE
      		DnDCursor(dndMove)
      		
      END
     type : 458752
   -
     name : DnD_Drop
     procedure_id : 1760454988220234794
     type_code : 14
     code : |1-
      // Summary: Procedure called during the drop in DnD mode
      // Syntax:
      //DnD_Drop ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE DnD_Drop()
      
      // Retrieves the subscript of the row
      nRowSubscript is int = TableInfoXY(TVT_Stream, tiLineNumber, MouseXPos(), MouseYPos())
      // The subscript must be valid
      IF nRowSubscript <= 0 THEN
      	DnDCursor(dndNone)
      	RETURN
      END
      
      nIDFolder is 8-byte int
      // According to the identifier corresponding to the drop, move the stream to the proper location
      SWITCH TVT_Stream.COL_ID[nRowSubscript]
      
      	// It is the "Subscriptions" folder
      	CASE SUBSCRIPTIONID
      		// The parent of the stream is the root folder
      		nIDFolder = 0
      		
      	// It is a folder
      	CASE [= PREFIX_FOLDER
      		// The parent of the stream is this folder
      		nIDFolder = ExtractString(TVT_Stream.COL_ID[nRowSubscript], 2, PREFIX_FOLDER)
      		
      	// It is a stream
      	OTHER CASE
      		// The stream has the same parent as this stream
      		nParentSubscript is int = TableGiveParent(TVT_Stream,nRowSubscript)
      		// The subscript must be valid
      		IF nParentSubscript <= 0 THEN
      			DnDCursor(dndNone)
      			RETURN
      		END
      		
      		// If the parent is the root folder
      		IF TVT_Stream.COL_ID[nParentSubscript] = SUBSCRIPTIONID THEN
      			nIDFolder = 0
      		ELSE // If the parent is a folder
      			nIDFolder = ExtractString(TVT_Stream.COL_ID[nParentSubscript], 2, PREFIX_FOLDER)
      		END
      		
      END
      
      // Modifies the stream
      HReadSeekFirst(RSSStream, RSSStreamID, gnMovedStreamID)
      IF HFound(RSSStream) = False THEN
      	DnDCursor(dndNone)
      	RETURN
      END
      RSSStream.FolderID = nIDFolder
      HModify(RSSStream)
      
      
      // Deletes the current line (the stream was moved)
      nSourceSubscript is int = TableSeek(TVT_Stream.COL_ID, gnMovedStreamID)
      // The subscript must be valid
      IF nSourceSubscript <= 0 THEN
      	DnDCursor(dndNone)
      	RETURN
      END
      // Deletes the initial line
      TableDelete(TVT_Stream, nSourceSubscript)
      
      // Refreshes the display of the stream
      RSSStream_Display(gnMovedStreamID)
      // Applies the count for each identifier
      CountApply_AnIdentifier(gnMovedStreamID, 0)	
      
      // Finds the stream
      nSubscript is int = TableSeek(TVT_Stream.COL_ID, gnMovedStreamID)
      IF nSubscript <= 0 THEN RETURN
      // Selects the stream
      TableSelectPlus(TVT_Stream, nSubscript)
      // Updates the GUI
      ExecuteProcess(TVT_Stream, trtSelection)
