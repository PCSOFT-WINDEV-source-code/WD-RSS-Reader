        code : |1-
         
         sName is string
         
         // Opens the window to enter a new folder
         EditName : 
         IF Input(<§$0054§>, sName, [<§$0055§>, <§$0056§>], 1, 2, dlgIconQuestion) = 2 THEN RETURN
         
         // The name must not be empty
         IF sName ~= "" THEN
         	Error("Enter a valid folder name (not empty)")
         	GOTO EditName
         END
         
         // Does the folder already exist?
         HSeekFirst(Folder, Caption, sName)
         IF HFound(Folder) THEN
         	
         	// Error Message
         	ErrorBuild(<§$0057§>, sName)
         	
         ELSE
         	
         	// Adds the folder in database
         	HReset(Folder)
         	Folder.Caption = sName
         	IF HAdd(Folder) = False THEN
         		Error(<§$0058§>, HErrorInfo(hErrMessage))
         		RETURN
         	END
         	
         	// Updates the treeview
         	FillTreeView_Folder(Folder.FolderID)
         	
         	// Sorts the Subscriptions branch
         	TableSortChild(TVT_Stream, 1, "COL_Sort")
         	
         END
