     name : FillTreeView_Folder
     procedure_id : 1760454988218924074
     type_code : 14
     code : |1-
      // Summary: Adds a "folder" branch
      // Syntax:
      //[ <Result> = ] FillTreeView_Folder (<nFolderID> is 8-byte int)
      //
      // Parameters:
      //	nFolderID (8-byte integer): Identifier of the folder
      // Return value:
      // 	integer: Subscript of the folder
      //
      PROCEDURE FillTreeView_Folder(LOCAL nIDFolder is 8-byte int) : int
      
      nFolderSubscript is int = -1
      nPosition is int = -1
      
      
      nPosition = HSavePosition(Folder, hSaveItems)
      
      // Reads the folder
      HReadSeekFirst(Folder, FolderID, nIDFolder)
      IF HFound(Folder) = False THEN RESULT -1
      
      // Finds the subscript of the folder
      nFolderSubscript = TableSeek(TVT_Stream.COL_ID, PREFIX_FOLDER+nIDFolder)
      
      // If the folder was not found yet (it was just added for example)
      IF nFolderSubscript <= 0 THEN
      	
      	// Defines the images for the new branch	
      	TVT_Stream..ExpandedImage = IMG_Opened_Directory
      	TVT_Stream..CollapsedImage = IMG_Closed_Directory
      	
      	// The folder is necessarily linked to the "subscriptions" folder
      	nParentSubscript is int = TableSeek(TVT_Stream.COL_ID, SUBSCRIPTIONID)
      	IF nParentSubscript <= 0 THEN RESULT -1
      	
      	// Adds the folder at the root
      	nFolderSubscript = TableAddChild(TVT_Stream, nParentSubscript, PREFIX_FOLDER+nIDFolder, Folder.Caption, 0, "0"+Folder.Caption, Folder.Caption)
      	
      END
      
      // Updates the stream elements that can change
      TVT_Stream[nFolderSubscript].COL_InitialCaption = Folder.Caption
      TVT_Stream[nFolderSubscript].COL_Sort 			= "0"+Folder.Caption
      
      // Sorts the Subscription branch
      TableSortChild(TVT_Stream, nParentSubscript, "COL_Sort")
      
      // Retrieves the subscript again (may have been modified by the sort)
      nFolderSubscript = TableSeek(TVT_Stream.COL_ID, PREFIX_FOLDER+nIDFolder)
      
      // Updates the folder counter
      //UPD_Folder_Counter(nFolderID)
      
      RESULT nFolderSubscript
      
      END : 
      IF nPosition <> -1 THEN HRestorePosition(nPosition)
