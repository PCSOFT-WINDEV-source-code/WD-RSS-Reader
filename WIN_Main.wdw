        internal_properties : CAAAAAgAAABrTXKJweYuBk/cb+qEz6ApQ53c7MiNc3tfUlNguKUPnyHMDrgDoDG9eDMTpKiGDV7X+ooAIHTaHOYlkEX4eAZc1Slcgx1UcDAMlkqk7rqAErOBYHsS0/6fKHA1TK9J/yx97c9ZeoPCMiS2Vww=
        code : |1-
         // Retrieves the subscript of the click
         nClickSubscript is int = TableInfoXY(TVT_Stream,tiLineNumber, MouseXPos(), MouseYPos())
         IF nClickSubscript <= 0 THEN RETURN
         
         // Selects the line that was clicked
         TableSelectPlus(TVT_Stream, nClickSubscript)
         
         // The menu to open depends on the click position
         SWITCH COL_ID[nClickSubscript]
         	// It's a folder
         	CASE [= PREFIX_FOLDER
         		// Opens the popup menu of folder
         		OpenPopupMenu(MENU_Folder)
         	
         	// It's the root
         	CASE SUBSCRIPTIONID
         		// Opens the popup menu of the root
         		OpenPopupMenu(MENU_Subscriptions)
         	
         	// "Standby" folder
         	CASE STANDBYID
         	
         	// Preset standby		
         	CASE NOTREADID, FAVORITESID, TODAYID, YESTERDAYID
         						
         	// Tags		
         	CASE [= PREFIX_TAG, TAGSID 			
         		// Opens the popup menu of tags
         		OpenPopupMenu(MENU_Tags)
         	
         	// It's a stream
         	OTHER CASE
         		// Opens the popup menu of streams
         		OpenPopupMenu(MENU_Stream)
         		
         END
