      // Summary: Customizes the display according to the options
      // Syntax:
      //CustomizeDisplay ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE CustomizeDisplay()
      
      // Displays/Hides the Share column of the table
      COL_Shared..Visible = OptionRead(OPTION_GUI_SHAREENABLED)
      // Displays/Hides the tag column of the table
      COL_Tag..Visible = OptionRead(OPTION_GUI_TAGENABLED)
      
      // Finds the "Tags" node
      nSubscript is int = TableSeek(COL_ID, TAGSID)
      // Displays/Hides the "Tags" node of the treeview
      IF OptionRead(OPTION_GUI_TAGENABLED) = True THEN
      	// If the row does not exist, add it
      	IF nSubscript <= 0 THEN FillTreeView_Tags()
      ELSE
      	// If the row exists, delete it
      	IF nSubscript > 0 THEN TableDelete(TVT_Stream, nSubscript)
      END
