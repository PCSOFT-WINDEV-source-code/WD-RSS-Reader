      // Summary: Updates the image of a stream in the GUI
      // Syntax:
      //RSSStream_UPD_Image (<nRSSStreamID> is 8-byte int)
      //
      // Parameters:
      //	nRSSStreamID (8-byte integer): Identifier of the RSS stream
      // Return value:
      // 	None
      //
      
      PROCEDURE RSSStream_UPD_Image(LOCAL nRSSStreamID is 8-byte int)
      
      nSubscript is int
      nPosition is int
      sStreamImage is string
      
      nPosition = HSavePosition(RSSStream, hSaveItems)
      
      // Positions on the stream
      HReadSeekFirst(RSSStream, RSSStreamID, nRSSStreamID)
      IF HFound(RSSStream) = False THEN RETURN
      
      // Finds the stream in the list
      nSubscript = FillTreeView_Stream(nRSSStreamID)
      // The subscript must be valid
      IF nSubscript <= 0 THEN RETURN
      
      // Default image
      sStreamImage = RSSStream_GetCacheImage(nRSSStreamID)
      // Modifies the image
      TVT_Stream[nSubscript]..ExpandedImage = sStreamImage
      TVT_Stream[nSubscript]..CollapsedImage = sStreamImage
      
      
      END:
      HRestorePosition(nPosition)
