     -
       name : OPT_Post_CopyShortcut
       identifier : 0x286e647c558b7c39
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1+
            sLinks is string
            
            // Browses the selected lines
            FOR ALL SELECTED ROW nSubscript of TABLE_Posts
            	
            	// Retrieves the post
            	stCurrentPost is STPost = TABLE_Posts[nSubscript]
            	
            	// Stores the link of the post
            	sLinks += [CR] + stCurrentPost.sLink
            	
            END
            
            // No link, no action
            IF sLinks ~= "" THEN RETURN
            
            // Copies the links of the posts into the clipboard
            ToClipboard(sLinks)
            
            // Informs the user
            sMessage is string
            IF TableSelectCount(TABLE_Posts) = 1 THEN
            	sMessage = <§$004e§>
            ELSE
            	sMessage = <§$004f§>
            END
            ToastDisplay(<§$002f§>+CR+sMessage, toastShort, vaMiddle, haCenter)
           type : 32
     -
       name : OPT_Separator7
       identifier : 0x286e647c558c7c39
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes : []
     -
       name : OPT_Post_Archive
       identifier : 0x286e647c558d7c39
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1+
            // At least one line must be selected
            IF TableSelectCount(TABLE_Posts) <= 0 THEN RETURN
            
            // Defines the question according to the number of selected lines
            sQuestion is string
            SWITCH TableSelectCount(TABLE_Posts)
            	CASE 1 : sQuestion = <§$004a§>
            	OTHER CASE : sQuestion = <§$0050§>
            END
            
            // Confirm
            IF Dialog(sQuestion+CR+<§$0051§>, [<§$004c§>, <§$002e§>], 1, 2, dlgIconQuestion) = 2 THEN RETURN
            
            bModification 	is boolean
            bArchive 		is boolean
            sReason 		is string
            sListReasons	is string
            // Browses the selected lines
            FOR ALL SELECTED ROW nSubscript of TABLE_Posts
            	
            	// Starts archiving the post
            	(bArchive, sReason) = Archiving_Post(TABLE_Posts.COL_PostID[nSubscript])
            	IF bArchive THEN
            		// At least one archiving was done
            		bModification = True
            	ELSE
            		// Stores the reason for the failure to display it to the user
            		sListReasons += [CR] + StringBuild("- ""%1"": %2",TABLE_Posts.COL_Title[nSubscript], sReason)
            	END
            	
            END
            
            // If a post is archived, refresh the display
            IF bModification THEN 
            	// Update all the counters
            	CountInitialize()
            	// Refreshes the display
            	WIN_Main.CountApply(0,1)
            	DisplaySelectionPosts()
            END
            
            // If failures occur, signal it to the user
            IF sListReasons <> "" THEN Info(<§$0052§>, sListReasons)
           type : 32
