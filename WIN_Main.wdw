            // Retrieves the identifier of the stream folder
            nIDFolder is 8-byte int
            IF TVT_Stream.COL_ID[TableGiveParent(TVT_Stream)] <> SUBSCRIPTIONID THEN
            	nIDFolder = Val(ExtractString(TVT_Stream.COL_ID[TableGiveParent(TVT_Stream)], 2, "_"))
            END
            
            // Opens the window for choosing folder
            IF Open(WIN_Stream_ChooseFolder, nRSSStreamID, nIDFolder) THEN
            	
            	// Deletes the current line (the stream was moved)
            	TableDelete(TVT_Stream)
            	
            	// Refreshes the display of the stream
            	RSSStream_Display(nRSSStreamID)
            	// Applies the count for each identifier
            	CountApply_AnIdentifier(nRSSStreamID, 0)	
            	
            	// Finds the stream
            	nSubscript is int = TableSeek(TVT_Stream.COL_ID, nRSSStreamID)
            	IF nSubscript <= 0 THEN RETURN
            	// Selects the stream
            	TableSelectPlus(TVT_Stream, nSubscript)
            	// Updates the GUI
            	ExecuteProcess(TVT_Stream, trtSelection)
            		
            END
