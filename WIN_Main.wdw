         // Stores the identifier of the post
         nPostID is 8-byte int = COL_PostID
         // Displays the preview of the post
         HReadSeekFirst(Post, PostID, nPostID)
         IF HFound(Post) THEN
         	
         	// Flags the post as read
         	IF COL_Read = False THEN
         		// Changes the flag in the table directly
         		COL_Read = True
         		// Refreshes the display
