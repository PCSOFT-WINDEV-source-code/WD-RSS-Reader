      // Summary: Event procedure on stream deletion
      // Syntax:
      //DeleteEvent (<nMessage>, <wParam>, <lParam>)
      //
      // Parameters:
      //	nMessage: Message
      //	wParam: Contains the identifier of the stream to update, 0 to update all the counters
      //	lParam: Not used
      // Return value:
      // 	None
      //
      PROCEDURE DeleteEvent(nMessage <useful>, wParam, lParam <useful>)
      
      // Stores the identifier of the stream
      nStreamID is 8-byte int = wParam
      
      // Finds the line of the stream
      nSubscript is int = TableSeek(TVT_Stream.COL_ID, nStreamID)
      IF nSubscript <= 0 THEN RETURN
      
      // Deletes the stream line
      TableDelete(TVT_Stream, nSubscript)
      
      // Refreshes the display
      DisplaySelectionPosts()
      CountInitialize()
      CountApply()
