      // Summary: Returns a message at the end of automatic UPD (if new streams are available)
      // Syntax:
      //AutomaticUPD_Message ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE AutomaticUPD_Message()
      
      sMessage is string
      
      
      // If the window is invisible only or if the user asked to always see the tooltip of new content
      IF gbWindowVisible _AND_ Val(OptionRead(OPTION_ALERT_ONLYIFREDUCED)) THEN RETURN
      
      // Retrieves all the posts whose receive date is greater than the date of last UPD
      QRY_ListPosts_Receive.pReceiveDate = gdtLastUPD
      IF HExecuteQuery(QRY_ListPosts_Receive) = False THEN RETURN
      
      // Browses the results
      FOR EACH QRY_ListPosts_Receive 
      	
      	// Adds a row
      	sMessage += [CR] + StringBuild(<§$000c§>, QRY_ListPosts_Receive.Title, QRY_ListPosts_Receive.NbPosts)
      	
      END
      
      // If a message must be displayed (= if new posts are found)
      IF sMessage <> "" THEN
      	
      	// Displays a tooltip indicating a new content
      	SysIconDisplayTooltip(sMessage, <§$000d§>, sysTooltipTypeDefault, "", SysTray_ClickTooltip)
      	
      END
