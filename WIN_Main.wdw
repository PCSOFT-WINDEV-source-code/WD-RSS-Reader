      // Summary: Displays/Hides the action buttons for the display in Looper mode
      // Syntax:
      //DisplayButtonsLOOPDisplay ([<bVisible> is boolean [, <nSubscript> is int]])
      //
      // Parameters:
      //	bVisible (boolean - default value=1): True to display the buttons, False otherwise
      //	nSubscript (integer): Subscript of buttons to display/hide
      // Return value:
      // 	None
      //
      PROCEDURE DisplayButtonsLOOPDisplay(LOCAL bVisible is boolean = True, LOCAL nSubscript is int = gnLOOPRolloverSubscript)
      
      // The subscript must be valid
      IF nSubscript <= 0 _OR_ nSubscript > LOOP_Posts..Count THEN RETURN
      
      // Changes the visibility of the "Favorites" button
      ATT_Favorites_Visible[nSubscript] = bVisible
      // Changes the tooltip of the "Favorites" button
      ATT_Favorites_Tooltip[nSubscript] = (ATT_Favorites[nSubscript] ? <§$0066§> ELSE <§$0067§>)
      
      // Changes the visibility of the "Archive" button
      // This button must not be visible if the post cannot be archived
      bArchivable is boolean
      sReason is string
      (bArchivable, sReason) = Archiving_PostIsArchivable(ATT_PostID[nSubscript])
      ATT_Archive_Visible[nSubscript] = (bVisible ? bArchivable ELSE False)
      
      IF OptionRead(OPTION_GUI_TAGENABLED) = True THEN ATT_Tags_Visible[nSubscript] = bVisible ELSE ATT_Tags_Visible[nSubscript] = False
      IF OptionRead(OPTION_GUI_SHAREENABLED) = True THEN ATT_Share_Visible[nSubscript] = bVisible ELSE ATT_Share_Visible[nSubscript] = False
      
