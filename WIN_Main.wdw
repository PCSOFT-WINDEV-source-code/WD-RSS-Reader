     name : DisplaySelectionPosts
     internal_properties : 
     procedure_id : 1760454988219251754
     type_code : 14
     code : |1+
      // Summary: Displays the posts found in the current treeview selection (and udpates the option tooltips)
      // Syntax:
      //DisplaySelectionPosts ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE DisplaySelectionPosts()
      
      WIN_Main..DisplayEnabled = False
      
      // Clears the table
      DeleteAll(garrPosts)
      
      // No parameter by default
      QRY_ListPosts.pFavorites = Null
      QRY_ListPosts.pIDFolder = Null
      QRY_ListPosts.pRSSStreamID = Null
      QRY_ListPosts.pIDTag = Null
      QRY_ListPosts.pDay = Null
      QRY_ListPosts.pLu = Null
      QRY_ListPosts.pListPostID = Null
      
      // Defines the default caption for the "Curent UPD" button
      BTN_UPD_Current..ToolTip = StringBuild(<§$000e§>, gFontBold(True)+<§$000f§>+gFontBold(False)) +CR+ <§$0010§>
      // Defines the default caption for "Flag read current"
      BTN_FlagRead_Current..ToolTip = StringBuild(<§$0011§>, gFontBold(True)+<§$000f§>+gFontBold(False)) +CR+ <§$0012§>
      
      // The menu to open depends on the click position
      SWITCH COL_ID[TVT_Stream]
      	// It's a folder
      	CASE [= PREFIX_FOLDER
      		// Displays all the posts of the folder streams
      		QRY_ListPosts.pIDFolder = ExtractString(TVT_Stream.COL_ID[TVT_Stream], 2,"_")
      		// Defines the text if the content is empty
      		STC_NoContent = StringBuild(<§$0013§>,TVT_Stream.COL_InitialCaption[TVT_Stream])
      		// Defines the caption for the "Curent UPD" button
      		BTN_UPD_Current..ToolTip = StringBuild(<§$0014§>, gFontBold(True)+TVT_Stream.COL_InitialCaption[TVT_Stream]+gFontBold(False)) +CR+ <§$0010§>
      		// Defines the caption for "Flag read current"
      		BTN_FlagRead_Current..ToolTip = StringBuild(<§$0015§>, gFontBold(True)+TVT_Stream.COL_InitialCaption[TVT_Stream]+gFontBold(False)) +CR+ <§$0012§>
      		
      	// It's the root
      	CASE SUBSCRIPTIONID
      		// Defines the text if the content is empty
      		STC_NoContent = <§$0016§>
      		// Defines the caption for the "Curent UPD" button
      		BTN_UPD_Current..ToolTip = <§$0017§>+gFontBold(True)+<§$000f§>+gFontBold(False) +CR+ <§$0010§>
      		// Defines the caption for "Flag read current"
      		BTN_FlagRead_Current..ToolTip = <§$0018§>+gFontBold(True)+<§$000f§>+gFontBold(False) +CR+ <§$0012§>
      		
      	// "Standby" folder
      	CASE STANDBYID
      		// Defines the standby text
      		STC_NoContent = <§$0064§>
      		// Displays the plane 3
      		WIN_Main..Plane = 3
      		RETURN
      		
      	// "Not read" folder
      	CASE NOTREADID
      		QRY_ListPosts.pLu = False
      		// Defines the text if the content is empty
      		STC_NoContent = <§$0019§>
      		
      	// "Favorites" folder
      	CASE FAVORITESID
      		QRY_ListPosts.pFavorites = True
      		// Defines the text if the content is empty
      		STC_NoContent = <§$001a§>
      		// Defines the caption for "Flag read current"
      		BTN_FlagRead_Current..ToolTip = StringBuild(<§$001b§>, gFontBold(True)+<§$001c§>+gFontBold(False)) +CR+ <§$0012§>
      		
      	// "Today" folder
      	CASE TODAYID
      		QRY_ListPosts.pDay = Today()
      		// Defines the text if the content is empty
      		STC_NoContent = <§$001d§>
      		// Defines the caption for "Flag read current"
      		BTN_FlagRead_Current..ToolTip = StringBuild(<§$001e§>, gFontBold(True)+<§$001f§>+gFontBold(False)) +CR+ <§$0012§>
      		
      	// "Yesterday" folder
      	CASE YESTERDAYID
      		QRY_ListPosts.pDay = Yesterday()
      		// Defines the text if the content is empty
      		STC_NoContent = <§$0020§>
      		// Defines the caption for "Flag read current"
      		BTN_FlagRead_Current..ToolTip = StringBuild(<§$001e§>, gFontBold(True)+<§$0021§>+gFontBold(False)) +CR+ <§$0012§>
      		
      	// Tags		
      	CASE [= PREFIX_TAG
      		// Displays all the posts flagged by the tag
      		QRY_ListPosts.pIDTag = ExtractString(TVT_Stream.COL_ID[TVT_Stream], 2,"_")
      		// Defines the text if the content is empty
      		STC_NoContent = StringBuild(<§$0022§>, TVT_Stream.COL_InitialCaption[TVT_Stream])
      		// Defines the caption for "Flag read current"
      		BTN_FlagRead_Current..ToolTip = StringBuild(<§$0023§>, gFontBold(True)+TVT_Stream.COL_InitialCaption[TVT_Stream]+gFontBold(False)) +CR+ <§$0012§>
      		
      	// "Tags" folder
      	CASE TAGSID
      		// Defines the standby text
      		STC_NoContent = <§$0065§>
      		// Displays the plane 3
      		WIN_Main..Plane = 3
      		RETURN
      		
      	// It's a stream
      	OTHER CASE
      		// Displays all the posts of the stream
      		QRY_ListPosts.pRSSStreamID = TVT_Stream.COL_ID[TVT_Stream]
      		// Defines the text if the content is empty
      		STC_NoContent = StringBuild(<§$0024§>, TVT_Stream.COL_InitialCaption[TVT_Stream])
      		// Defines the caption for the "Curent UPD" button
      		BTN_UPD_Current..ToolTip = StringBuild(<§$0025§>, gFontBold(True)+TVT_Stream.COL_InitialCaption[TVT_Stream]+gFontBold(False)) +CR+ <§$0010§>
      		// Defines the caption for "Flag read current"
      		BTN_FlagRead_Current..ToolTip = StringBuild(<§$0026§>, gFontBold(True)+TVT_Stream.COL_InitialCaption[TVT_Stream]+gFontBold(False)) +CR+ <§$0012§>
      END
      
      // Runs the query
      HExecuteQuery(QRY_ListPosts)
      
      // Loads the results in the array
      FileToArray(garrPosts, QRY_ListPosts)
      
      // Stores the selections (for the table, the storage is performed manually because we don't want to restore the multi-selection)
      nSelectionSubscript is int = TableSelect(TABLE_Posts, 1)
      nPositionSubscript is int = TablePosition(TABLE_Posts)
      LooperSavePositionAndSelection(LOOP_Posts)
      
      // Refreshes the table
      TableDisplay(TABLE_Posts, taInit)
      // Refreshes the looper
      LooperDisplay(LOOP_Posts, taInit)
      
      // Clears the preview
      HTM_Preview = ""
      
      // Repositions the selections
      nSelectionSubscript = Min(nSelectionSubscript, TABLE_Posts..Count)
      nPositionSubscript = Min(nPositionSubscript, TABLE_Posts..Count)
      IF nSelectionSubscript > 0 THEN
      	// Repositions at the proper location
      	TablePosition(TABLE_Posts, nPositionSubscript)
      	// Selects the row
      	TableSelectPlus(TABLE_Posts, nSelectionSubscript)
      	// Triggers the selection
      	ExecuteProcess(TABLE_Posts, trtSelection)
      END
      LooperRestorePositionAndSelection(LOOP_Posts)
      
      // If no post is listed
      IF garrPosts..Count = 0 THEN
      	// Displays the plane 3
      	WIN_Main..Plane = 3
      ELSE
      	// Repositions the plane if necessary
      	IF WIN_Main..Plane = 3 THEN WIN_Main..Plane = OptionRead(OPTION_GUI_MODE)
      END
      
      END:
      WIN_Main..DisplayEnabled = True
     type : 458752
