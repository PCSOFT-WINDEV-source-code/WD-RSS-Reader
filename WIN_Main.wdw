        internal_properties : CAAAAAgAAAAH8cZNmdrgghtwKz54wxD5vyGUwFgRkwfjHg+EtKHn9zU4rgz7hMmR7D9vYPTCtW7DfgJMABjoCpC/Jkva9pT231cKeVdKdjYGgLAqUEgOnJ2P7sHkQYQVUkJTWnnvXUY3e8W39DOSZmhK+6A=
        code : |1+
         arrStreamID is array of 8-byte int
         
         // If an update is already in progress
         IF gbUPDInProg THEN
         	// Don't restart an update
         	ToastDisplay(<§$0059§>, toastShort, vaMiddle, haCenter)
         	RETURN
         END
         
         // According to the current selection
         SWITCH COL_ID[TVT_Stream]
         	// It's a folder
         	CASE [= PREFIX_FOLDER
         		// Retrieves the folder identifier
         		nIDFolder is 8-byte int = ExtractString( TVT_Stream.COL_ID[TVT_Stream], 2, PREFIX_FOLDER)
         		// Browses the folder streams
         		FOR EACH RSSStream WHERE FolderID = nIDFolder		
         			// Stores the stream
         			Add(arrStreamID, RSSStream.RSSStreamID)
         		END
         		
         	// It's the root, a standby folder or a tag folder
         	CASE SUBSCRIPTIONID,STANDBYID, NOTREADID, FAVORITESID, TODAYID, YESTERDAYID, [= PREFIX_TAG, TAGSID
         		// Browses the streams
         		FOR EACH RSSStream
         			// Stores the stream
         			Add(arrStreamID, RSSStream.RSSStreamID)
         		END
         
         	// It's a stream
         	OTHER CASE
         		// Retrieves the identifier of the current stream
         		nRSSStreamID is 8-byte int = TVT_Stream.COL_ID[TVT_Stream]
         		// Stores the stream
         		Add(arrStreamID, nRSSStreamID)
         		
         END
         
         // Starts the procedure (thread) that will start the recovery tasks
         UPDStart(arrStreamID, False)
