     type_code : 4
     p_codes :
      -
        code : |1+
         nIDFolder is 8-byte int
         nSubscript is int
         
         // If the current selection is a stream
         IF IsNumeric(TVT_Stream.COL_ID[TVT_Stream]) THEN
         	
         	// Retrieves the parent folder
         	nSubscript = TableGiveParent(TVT_Stream, TVT_Stream)
         	// If it is a folder
         	IF TVT_Stream.COL_ID[nSubscript] [= PREFIX_FOLDER THEN
         		// Retrieves its identifier
         		nIDFolder = Val(ExtractString(TVT_Stream.COL_ID[nSubscript], 2, "_"))
         	ELSE
         		// Otherwise, it is the "Subscriptions" folder
         		nIDFolder = 0
         	END
         	
         ELSE // If the current selection is not a stream
         	
         	// Retrieves the identifier of the current or parent folder
         	SWITCH TVT_Stream.COL_ID[TVT_Stream]
         		// "Subscriptions" folder
         		CASE SUBSCRIPTIONID : nIDFolder = 0
         			
         		// Stream folder
         		CASE [= PREFIX_FOLDER : nIDFolder = Val(ExtractString(TVT_Stream.COL_ID[TVT_Stream], 2, "_"))
         			
         		// Other folders (tags, standby, ...)
         		OTHER CASE : nIDFolder = 0
         			
         	END
         	
         END
         
         // Opens the window for entering a new stream
         nStreamID is 8-byte int
         nStreamID = Open(WIN_NewStream, nIDFolder)
         IF nStreamID > 0 THEN
         	// Updates the stream
         	UPDStart([nStreamID], False)
         END
        type : 18
