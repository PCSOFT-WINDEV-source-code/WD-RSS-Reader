      // Summary: Displays the current post in the ActivX
      // Syntax:
      // DisplayPost ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE DisplayPost()
      
      HTM_Preview = ""
      
      // Template used to standardize the style of the preview
      sModel is string = [
      	<HTML>
      	<STYLE>
      	body {
      	font-size: small;
      	font-family: "Segoe UI", "Trebuchet MS", Tahoma, sans-serif;
      	margin: 14px 18px;
      	color: black;
      	background-color: #fff;
      	}
      	</STYLE>
      	<BODY>
      	%1
      	</BODY>
      	</HTML>
      ]
      
      // The content must be checked (especially the iframe)
      sPostContent is string = Post.Content
      sSiteURL is string
      HReadSeekFirst(RSSStream, RSSStreamID, Post.RSSStreamID)
      IF HFound(RSSStream) THEN sSiteURL = RSSStream.Link
      // The iframe ("src") and the "script" must be processed
      sPostContent = DisplayPost_CorrectContent(sPostContent, sSiteURL)
      
      
      // Declares a temporary file for the preview
      sPreviewFile is string = SET_Global.gsPreviewDirectory +["\"]+ StringBuild(<§$006e§>, Post.PostID)
      // Creates the file if it does not exist
      IF fFileExist(sPreviewFile) = False THEN
      	// Saves the content for the preview
      	fSaveText(sPreviewFile, StringBuild(sModel, sPostContent))
      END
      // Displays the content of the post
      HTM_Preview = sPreviewFile
     type : 458752
