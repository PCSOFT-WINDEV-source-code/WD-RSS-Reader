        code : |1+
         // Retrieves the hovered column
         sColumn is string = TableInfoXY(MySelf, tiColName, MouseXPos(), MouseYPos())
         // Retrieves the hovered row
         nSubscript is int = TableInfoXY(MySelf, tiLineNumber, MouseXPos(), MouseYPos())
         IF nSubscript <= 0 THEN RETURN
         // If we hover the same row and the same column, no action
         IF nSubscript = gnRolloverSubscript _AND_ sColumn = gsRolloverColumn THEN RETURN
         // Stores the row and the column
         gnRolloverSubscript = nSubscript
         gsRolloverColumn = sColumn
         
         // Adapts the tooltip to the column
         SWITCH sColumn
         	// Favorites
         	CASE COL_FavoritesImage..Name
         		MySelf..ToolTip = (COL_Favorites[nSubscript] ? <§$0066§> ELSE <§$0067§>)
         		
         	// Shared
         	CASE COL_Shared..Name
         		MySelf..ToolTip = <§$0045§>
         		
         	// Tags
         	CASE COL_Tag..Name
         		MySelf..ToolTip = <§$0046§>
         		// Displays the tags already assigned
         		IF TABLE_Posts[nSubscript].COL_AssociatedTags <> "" THEN
         			MySelf..ToolTip += CR+CR+ gFontItalic(True)+gPen(HTMLToRGB("#2196F3")) + Replace(TABLE_Posts[nSubscript].COL_AssociatedTags, ";", CR)
         		END
         		
         	// Other columns
         	OTHER CASE
         		// Retrieves the post
         		stAPost is STPost = TABLE_Posts[nSubscript]
         		// By default, displays the title and the description (the beginning)
         		sDescription is string = HTMLToText(stAPost.sDescription)
         		MySelf..ToolTip = gFontBold(True)+stAPost.sTitle+gFontBold(False) +CR+CR+ Left(sDescription, 200) + (Length(sDescription) > 200 ? "..." ELSE "") +CR+CR+ gFontItalic(True)+gPen(HTMLToRGB("#2196F3"))+TABLE_Posts[nSubscript].COL_AssociatedTags
         END
