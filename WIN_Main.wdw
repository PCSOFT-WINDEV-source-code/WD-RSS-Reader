        code : |1-
         // Retrieves the subscript of the click
         nRolloverSubscript is int = TableInfoXY(TVT_Stream,tiLineNumber, MouseXPos(), MouseYPos())
         // The subscript must be valid
         IF nRolloverSubscript <= 0 THEN
         	MySelf..ToolTip = ""
         	RETURN
         END
         // The subscript must differ from the previous one
         IF nRolloverSubscript = gnListRolloverSubscript THEN RETURN
         
         // Stores the current rollover subscript
         gnListRolloverSubscript = nRolloverSubscript
         
         sToolTip is string
         
         // The tooltip depends on the rollover position
         SWITCH COL_ID[nRolloverSubscript]
         	// It's a folder
         	CASE [= PREFIX_FOLDER : sToolTip = StringBuild(<§$0031§>, TVT_Stream.COL_InitialCaption[nRolloverSubscript])
         	
         	// It's the root
         	CASE SUBSCRIPTIONID : sToolTip = <§$0032§>+CR+<§$0033§>
         		
         	// "Standby" folder
         	CASE STANDBYID :  sToolTip = <§$0034§>+CR+<§$0035§>
         		
         	// Not read
         	CASE NOTREADID : sToolTip = <§$0036§>+CR+<§$0037§>
         	
         	// Favorites
         	CASE FAVORITESID : sToolTip = <§$0038§>+CR+<§$0039§>
         		
         	// Today
         	CASE TODAYID : sToolTip = <§$003a§>+CR+<§$003b§>
         	
         	// Yesterday
         	CASE YESTERDAYID : sToolTip = <§$003c§>+CR+<§$003d§>
         		
         	// Tags		
         	CASE [= PREFIX_TAG : sToolTip = StringBuild(<§$003e§>+CR+<§$003f§>, TVT_Stream.COL_InitialCaption[nRolloverSubscript])
         	
         	// List of tags
         	CASE TAGSID : sToolTip = <§$0040§>+CR+<§$0041§>
         
         	// It's a stream
         	OTHER CASE
         		sToolTip = <§$0042§>+TVT_Stream.COL_InitialCaption[nRolloverSubscript]
         END
         
         // Assigns the tooltip
         MySelf..ToolTip = sToolTip
