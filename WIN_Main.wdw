      // Summary: Procedure used to automatically update the streams in a given period
      // Syntax:
      //AutoRefresh ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      PROCEDURE AutoRefresh()
      
      arrStream is array of 8-byte int
      
      // By default, the automatic UPD retrieves all the streams
      // (with parameters for each stream, we could avoid updating some streams)
      FOR EACH RSSStream 
      	Add(arrStream, RSSStream.RSSStreamID)
      END
      
      // Updates the content of all streams
      UPDStart(arrStream, True)
      
      // Stops the timer
      EndTimerSys(TIMER_AUTOUPD)
      
      // Retarts the timer for automatic update
      TimerSys(AutoRefresh, Val(OptionRead(OPTION_ALERT_TEMPORIZATION))*60*100, TIMER_AUTOUPD)
