      // Summary: Procedure called when a tag is checked/unchecked
      // Syntax:
      //Tags_Check (<nTagID> is 8-byte int)
      //
      // Parameters:
      //	nTagID (8-byte integer): Identifier of the tag
      // Return value:
      // 	None
      //
      PROCEDURE Tags_Check(LOCAL nTagID is 8-byte int)
      
      // Stores the identifier of the post
      nPostID is 8-byte int = gnCurrentPostID
      IF gnCurrentPostID <= 0 THEN RETURN
      
      // Adds or deletes the association
      HReadSeekFirst(Association,AssociationID,[nPostID, nTagID])
      IF HFound(Association) THEN
      	HDelete(Association)
      ELSE
      	HReset(Association)
      	Association.PostID = nPostID
      	Association.TagID = nTagID
      	HAdd(Association)
      END
      
      // Updates the counter for this tag
      UPD_Counter_Tag(nTagID)
      CountApply_AnIdentifier(PREFIX_TAG+nTagID, False)
      
      // Refreshes the display of the row
      nSubscript is int
      sAssociatedTags is string = Post_ListAssociatedTags(gnCurrentPostID)
      
      nSubscript = TableSeek(COL_PostID, gnCurrentPostID)
      IF nSubscript > 0 THEN COL_AssociatedTags[nSubscript] = sAssociatedTags ; TableDisplay(TABLE_Posts, nSubscript)
      
      nSubscript = LooperSeek(ATT_PostID, gnCurrentPostID)
      IF nSubscript > 0 THEN ATT_AssociatedTags[nSubscript] = sAssociatedTags ; LooperDisplay(LOOP_Posts, nSubscript)
