        code : |1-
         IF CurrentSubscript <= 0 THEN RETURN
         
         // If the post was not read, it must be displayed in bold
         TABLE_Posts[CurrentSubscript].COL_Title..FontBold = (COL_Read = False)
         
         // Stream image
         IF TABLE_Posts[CurrentSubscript].COL_StreamImage ~= "" THEN TABLE_Posts[CurrentSubscript].COL_StreamImage = RSSStream_GetCacheImage(COL_StreamID)
         
         // Associated tags
         TABLE_Posts[CurrentSubscript].COL_AssociatedTags = post_ListAssociatedTags(TABLE_Posts[CurrentSubscript].COL_PostID)
         
         // Buttons:
         // -  Favorites
         TABLE_Posts[CurrentSubscript].COL_FavoritesImage = (TABLE_Posts[CurrentSubscript].COL_Favorites ? IMG_FavoritesFull ELSE IMG_Favorites)
         TABLE_Posts[CurrentSubscript].COL_Title..Color = (TABLE_Posts[CurrentSubscript].COL_Favorites ? HTMLToRGB("#E91E63") ELSE Black)
         // - Share
         TABLE_Posts[CurrentSubscript].COL_Shared = IMG_Share
         // - Tags
         TABLE_Posts[CurrentSubscript].COL_Tag = (TABLE_Posts[CurrentSubscript].COL_AssociatedTags ~= "" ? IMG_TAG ELSE IMG_TAG_Blue)
