     code : |1+
      // Summary: Customizes the menu for a post
      // Syntax:
      //Tags_CustomizeMenuForPost (<nPostID> is 8-byte int)
      //
      // Parameters:
      //	nPostID (8-byte integer): Identifier of the post
      // Return value:
      // 	None
      //
      PROCEDURE Tags_CustomizeMenuForPost(LOCAL nPostID is 8-byte int)
      
      arrCategories is array of strings
      nExistingTagSubscript is int
      
      
      // Finds the post
      HReadSeekFirst(Post, PostID, nPostID)
      IF HFound(Post) = False THEN RETURN
      
      // Retrieves the categories of the post
      StringToArray(Post.Category, arrCategories,";")
      ArraySort(arrCategories, asAscending)
      
      // Delete the previous suggestions
      nSubscript is int = 1
      sOption is string = OPTIONSUGGTAGS+nSubscript
      WHILE MenuExist(MENU_Post_Tag, sOption)
      	// Deletes the option
      	MenuDelete(sOption)
      	// Next option
      	nSubscript++
      	sOption = OPTIONSUGGTAGS+(nSubscript)
      END
      
      
      // For each tag
      nExistingTagSubscript = 1
      FOR EACH Tag on Caption
      
      	// Stores the tag subscript in the suggestions
      	nSubscript = Seek(arrCategories, asLinear, Tag.Caption)
      	
      	// Defines the name of the option
      	sOption =  OPTIONTAGS+nExistingTagSubscript
      	
      	// If an association exists for this tag
      	HSeekFirst(Association, AssociationID,[nPostID, Tag.TagID])
      	IF HFound(Association) THEN 
      		// Checks the tag and switches it to bold
      		{sOption, indControl}..Image = IMG_FullCheckmark
      		{sOption, indControl}..FontBold = True
      		
      	ELSE
      		// Tag not associated
      		{sOption, indControl}..Image = IMG_EmptyCheckmark
      		{sOption, indControl}..FontBold = False
      		
      		// If this tag is a proposal, it must be highlighted (in bold and in blue)
      		IF nSubscript > 0 THEN
      			{sOption, indControl}..Color = HTMLToRGB("#2196F3")
      		END
      		
      	END
      	
      	// If this tag is a proposal, it must be removed from the list of proposals
      	IF nSubscript > 0 THEN Delete(arrCategories, nSubscript)
      	
      	// Next subscript
      	nExistingTagSubscript++
      
      END
      
      
      nExistingTagSubscript = 1
      // Browses the suggestions (sorted in alphabetical order)
      FOR ALL ELEMENT sASuggestion, nSubscript of arrCategories
      	
      	// The suggestion must not be empty
      	IF sASuggestion IN ("", EOT) THEN CONTINUE
      	
      	// Moves forward among the existing options
      	WHILE MenuExist(MENU_Post_Tag, OPTIONTAGS+nExistingTagSubscript) _AND_ Lower({ OPTIONTAGS+nExistingTagSubscript, indControl}..Caption) < Lower(sASuggestion)
      		nExistingTagSubscript++
      	END
      
      	// Defines the name of the option
      	sOption = OPTIONSUGGTAGS+nSubscript
      	
      	IF MenuExist(MENU_Post_Tag,  OPTIONTAGS+nExistingTagSubscript) THEN
      		// Inserts the option
      		MenuInsertOption(MENU_Post_Tag,  OPTIONTAGS+nExistingTagSubscript, sOption, sASuggestion, Tags_Add, sASuggestion)
      	ELSE
      		// Adds the option
      		MenuAddOption(MENU_Post_Tag, sOption, sASuggestion, Tags_Add, sASuggestion)
      	END
      	// Checks empty
      	{sOption, indControl}..Image = IMG_EmptyCheckmark
      	// Suggestion, therefore in blue
      	{sOption, indControl}..Color = HTMLToRGB("#2196F3")
      	
      END
