      // Summary: Fills the tags
      // Syntax:
      //FillTreeView_Tags ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE FillTreeView_Tags()
      
      nRootSubscript is int
      sSelectID is string
      
      // Only if the tags are active
      IF OptionRead(OPTION_GUI_TAGENABLED) = False THEN RETURN
      
      // If the current selection is a tag or the root of tags
      IF TVT_Stream.COL_ID [= PREFIX_TAG _OR_ TVT_Stream.COL_ID = TAGSID THEN
      	// Stores the selected identifier
      	sSelectID = TVT_Stream.COL_ID
      END
      
      // Defines the image
      TVT_Stream..ExpandedImage = IMG_TAG_TreeView
      TVT_Stream..CollapsedImage = IMG_TAG_TreeView
      
      // Finds the root of tags
      nRootSubscript = TableSeek(TVT_Stream.COL_ID, TAGSID)
      IF nRootSubscript <= 0 THEN
      	// Adds the root if necessary
      	nRootSubscript = TableAddLine(TVT_Stream, TAGSID, <§$0004§>, 0, "3", <§$0004§>)
      END
      
      // Clears the children
      TableDeleteChild(TVT_Stream, nRootSubscript)
      
      // Adds the children
      FOR EACH Tag on Caption
      	// Adds the tag
      	TableAddChild(TVT_Stream, nRootSubscript, PREFIX_TAG+Tag.TagID, Tag.Caption, 0, Tag.Caption, Tag.Caption)
      	// Updates the counter
      	CountApply_AnIdentifier(PREFIX_TAG+Tag.TagID)
      END
      
      // Expands the node
      TableExpand(TVT_Stream, nRootSubscript)
      
      // Repositions the previous selection
      IF sSelectID <> "" THEN
      	nSubscript is int = TableSeek(TVT_Stream.COL_ID, sSelectID)
      	IF nSubscript > 0 THEN
      		TableSelectPlus(TVT_Stream, nSubscript)
      	END
      END
