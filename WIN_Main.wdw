      // Summary: Applies the theme color
      // Syntax:
      //ThemeApply ([<sThemeColor> is string])
      //
      // Parameters:
      //	sThemeColor (ANSI string): Color of the theme
      // Return value:
      // 	None
      //
      PROCEDURE ThemeApply(sThemeColor is string = OptionRead(OPTION_GUI_COLOR))
      
      // Retrieves the theme color
      nColor is int = HTMLToRGB(sThemeColor)
      
      // If the color is the one that is already applied, no action
      IF IMG_LeftLayout..BackgroundColor = nColor THEN RETURN
      
      AnimationPrepare(IMG_LeftLayout)
      // Applies the color to the background of the layout image
      IMG_LeftLayout..BackgroundColor = nColor
      // Modifies the writing color
      TVT_Stream..Color = ReadableColor(nColor)
      AnimationPlay(animFadeIn)
