    options :
     -
       name : OPT_Post_OpenBrowser
       identifier : 0x286e647c55897c39
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1+
            
            // Browses the selected lines
            FOR ALL SELECTED ROW nSubscript of TABLE_Posts
            	
            	// Starts the default browser on the address of the post
            	Post_OpenInBrowser(TABLE_Posts.COL_PostID[nSubscript])
            	
            END
           type : 32
