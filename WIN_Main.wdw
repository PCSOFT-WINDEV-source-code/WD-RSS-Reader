      // Summary: Returns the reverse color adapted to the requested color
      // Syntax:
      //[ <Result> = ] ReadableColor (<InitialColor> is Color)
      //
      // Parameters:
      //	InitialColor (Color): Initial color
      // Return value:
      // 	Color: Reverse color
      //
      PROCEDURE ReadableColor(LOCAL InitialColor is a Color) : Color
      
      nLuminance is int
      rBrightness is real
      ReturnColor is a Color
      
      
      // Calculate the lightness of the color passed in parameter
      rBrightness = 1 - (0.299 * InitialColor..Red + 0.587 * InitialColor..Green + 0.114 * InitialColor..Blue)/255
      
      // According to the luminance, the text color must have a low or high lightness
      IF rBrightness < 0.5 THEN
      	// Low luminosity (recommended values: 0-20 | a value close to 0 means that the text is black ; a value near 20 means that the text is colored)
      	nLuminance = 20
      	ReturnColor = Black
      ELSE
      	// High luminosity (recommended values: 80-100 | a value close to 100 means that the text is white ; a value close to 80 means that the text is colored)
      	nLuminance = 80 
      	ReturnColor = White
      END
      
      // The returned color keeps the shade and the saturation
      //ColorReturn = HSL(ColorHue(InitialColor), CouleurSaturation(InitialColor), nLightness)
      
      
      // Returns the reverse color found
      RESULT ReturnColor
     type : 458752
