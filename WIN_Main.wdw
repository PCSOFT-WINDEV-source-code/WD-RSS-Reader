        internal_properties : CAAAAAgAAADEA/DuiaEWCDfWS26uA0wb70CVKRhhlJbNcz/rgXOOywPIUE3hgUdHwoITMXF+/7jzgGuz10P/9v1KeBOFHk3BWkUYRfCkjSJrCxiOW54LI7BPckv7a/c9Jbc1tKoNyL/ME6FVpOPud/QdNQ==
        code : |1-
         // Defines the default caption for "Flag read current"
         MySelf..ToolTip = StringBuild(<§$0011§>, gFontBold(True)+<§$000f§>+gFontBold(False)) +CR+ <§$0012§>
        type : 14
      -
        code : |1+
         MyTask is a ParallelTask
         
         // According to the current selection
         SWITCH COL_ID[TVT_Stream]
         	// It's a folder
         	CASE [= PREFIX_FOLDER
         		// Confirm
         		IF Val(OptionRead(OPTION_CONFIRMFLAGREAD)) _AND_ Dialog(StringBuild(<§$005a§>, TVT_Stream.COL_InitialCaption[TVT_Stream]), [<§$005b§>,<§$002e§>], 1, 2, dlgIconQuestion) = 2 THEN RETURN
         		// Retrieves the folder identifier
         		nIDFolder is 8-byte int = ExtractString(TVT_Stream.COL_ID[TVT_Stream], 2, PREFIX_FOLDER)
         		// Flags the folder streams as read
         		MyTask = ParallelTaskExecute(FlagRead_Folder, (nIDFolder))
         
         	// It's a root or the standby of all unread
         	CASE SUBSCRIPTIONID, STANDBYID, TAGSID, NOTREADID
         		// Confirm
         		IF Val(OptionRead(OPTION_CONFIRMFLAGREAD)) _AND_ Dialog(<§$005c§>, [<§$005b§>,<§$002e§>], 1, 2, dlgIconQuestion) = 2 THEN RETURN
         		// Flags all the posts as read
         		ParallelTaskExecute(FlagRead_All, ())
         		
         	// It's the "favorites" folder
         	CASE FAVORITESID
         		// Confirm
         		IF Val(OptionRead(OPTION_CONFIRMFLAGREAD)) _AND_ Dialog(<§$005d§>, [<§$005b§>,<§$002e§>], 1, 2, dlgIconQuestion) = 2 THEN RETURN
         		// Flags all the favorite posts as read
         		ParallelTaskExecute(FlagRead_Favorites, ())
         		
         		
         	// It's the "today" folder
         	CASE TODAYID
         		// Confirm
         		IF Val(OptionRead(OPTION_CONFIRMFLAGREAD)) _AND_ Dialog(<§$005e§>, [<§$005b§>,<§$002e§>], 1, 2, dlgIconQuestion) = 2 THEN RETURN
         		// Flags all the today posts as read
         		ParallelTaskExecute(FlagRead_Today, ())
         		
         	// It's the "yesterday" folder
         	CASE YESTERDAYID
         		// Confirm
         		IF Val(OptionRead(OPTION_CONFIRMFLAGREAD)) _AND_ Dialog(<§$005f§>, [<§$005b§>,<§$002e§>], 1, 2, dlgIconQuestion) = 2 THEN RETURN
         		// Flags all the yesterday posts as read
         		ParallelTaskExecute(FlagRead_Yesterday, ())
         		
         	// It's a custom tag
         	CASE [= PREFIX_TAG
         		// Confirm
         		IF Val(OptionRead(OPTION_CONFIRMFLAGREAD)) _AND_ Dialog(StringBuild(<§$0060§>, COL_InitialCaption[TVT_Stream]), [<§$005b§>,<§$002e§>], 1, 2, dlgIconQuestion) = 2 THEN RETURN
         		// Retrieves the identifier of the tag
         		nTagID is 8-byte int = ExtractString(COL_ID, 2, PREFIX_TAG)
         		// Flags all the posts associated with a tag as read
         		ParallelTaskExecute(FlagRead_Tags, (nTagID))
         		
         	// It's a stream
         	OTHER CASE
         		
         		// Confirm
         		IF Val(OptionRead(OPTION_CONFIRMFLAGREAD)) _AND_ Dialog(StringBuild(<§$0061§>, TVT_Stream.COL_InitialCaption[TVT_Stream]), [<§$005b§>,<§$002e§>], 1, 2, dlgIconQuestion) = 2 THEN RETURN
         		// Retrieves the identifier of the current stream
         		nRSSStreamID is 8-byte int = TVT_Stream.COL_ID[TVT_Stream]
         		// Flags the stream as read
         		MyTask = ParallelTaskExecute(FlagRead_RSSStream, (nRSSStreamID))
         		
         END
