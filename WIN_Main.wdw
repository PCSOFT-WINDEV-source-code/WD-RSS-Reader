     name : DisplayPost_CorrectContent
     internal_properties : 
     procedure_id : 1760454988220431402
     type_code : 14
     code : |1-
      // Summary: Corrects the content of a post to avoid the display problems
      // Syntax:
      //[ <Result> = ] DisplayPost_FixContent (<sPostContent> is string, <sSiteURL> is string)
      //
      // Parameters:
      //	sPostContent (ANSI string): Content of the post
      //	sSiteURL (ANSI string): URL of corresponding site
      // Return value:
      // 	ANSI string: Fixed content
      //
      PROCEDURE PRIVATE DisplayPost_CorrectContent(LOCAL sPostContent is string, LOCAL sSiteURL is string) : string
      
      nTagStart, nTagEnd are int
      nSRCStart, nSRCEnd are int
      sPostContentFixed is string
      sSRC is string
      
      
      // Stores the initial content
      sPostContentFixed = sPostContent
      
      // Deletes the scripts
      sPostContentFixed = ReplaceWildcard(sPostContentFixed, "<script*/script>", "", IgnoreCase)
      // Deletes the iframe
      sPostContentFixed = ReplaceWildcard(sPostContentFixed, "<iframe*/iframe>", "", IgnoreCase)
      
      // Correct the src attributes on the images
      arrTags is array of strings = ["img"]
      FOR ALL ELEMENT sATag of arrTags
      	
      	// Redefines the URL of images whose path is relative
      	nTagStart = Position(sPostContentFixed, "<"+sATag, 1, IgnoreCase)
      	WHILE nTagStart > 0
      		// Finds the closing of the tag
      		nTagEnd = Position(sPostContentFixed, ">", nTagStart)
      		// No closing, we must stop
      		IF nTagEnd = 0 THEN BREAK
      		// Extracts the full iframe tag
      		sImage is string = sPostContentFixed[[nTagStart TO nTagEnd]]
      		
      		// Finds the src
      		nSRCStart = Position(sImage, "src=""",1, IgnoreCase)
      		// If the src is found
      		IF nSRCStart > 0 THEN
      			nSRCStart += 5
      			// Finds the end of the link
      			nSRCEnd = Position(sImage, """", nSRCStart)
      			// If the end is found
      			IF nSRCEnd > 0 THEN
      				nSRCEnd--
      				// Extracts the link of the src attribute
      				sSRC = sImage[[nSRCStart TO nSRCEnd]]
      				
      				// If the link starts with "//", forces the http (otherwise, the ActiveX will not find it)
      				IF sSRC [= "//" THEN
      					// Adds http:
      					sSRC = "http:"+sSRC
      					// Corrects the link in the iframe
      					sImage[[nSRCStart TO nSRCEnd]] = sSRC
      					// Corrects the link in the content
      					sPostContentFixed[[nTagStart TO nTagEnd]] = sImage
      				ELSE
      					// If the lilnk starts with a simple "/", the absolute path must be used
      					IF sSRC [= "/" THEN
      						// Adds http:
      						sSRC = sSiteURL+["/"]+sSRC
      						// Corrects the link in the iframe
      						sImage[[nSRCStart TO nSRCEnd]] = sSRC
      						// Corrects the link in the content
      						sPostContentFixed[[nTagStart TO nTagEnd]] = sImage
      					END
      				END
      				
      			END
      			
      		END
      		
      		// Finds another <img> tag in the content
      		nTagStart = Position(sPostContentFixed, "<"+sATag, nTagEnd, IgnoreCase)
      		
      	END
      	
      END
      
      
      // Returns the corrected post
      RESULT sPostContentFixed
