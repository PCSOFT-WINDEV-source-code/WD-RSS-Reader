   -
     name : FillThemes
     procedure_id : 1760455001105860109
     type_code : 14
     code : |1-
      // Summary: Fills the list of themes
      // Syntax:
      // FillThemes ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE FillThemes()
      
      
      // Adds the available colors
      Add(garrTheme,"#212121")	// Black
      Add(garrTheme,"#515151")	// Black (default)
      Add(garrTheme,"#BDBDBD")	// Gray
      
      Add(garrTheme,"#E53935")	// Red
      Add(garrTheme,"#D81B60")	// Pink
      Add(garrTheme,"#8E24AA")	// Purple
      
      Add(garrTheme,"#3949AB")	// Indigo
      Add(garrTheme,"#1E88E5")	// Blue
      Add(garrTheme,"#00ACC1")	// Cyan
      
      Add(garrTheme,"#43A047")	// Green
      Add(garrTheme,"#7CB342")	// Light green
      Add(garrTheme,"#C0CA33")	// Lemon
      
      Add(garrTheme,"#FDD835")	// Yellow
      Add(garrTheme,"#FB8C00")	// Orange
      Add(garrTheme,"#F4511E")	// Dark Orange
      
      // Clears the control
      LooperDeleteAll(LOOP_Theme)
      
      // Fills the themes
      sColor is string
      FOR ALL ELEMENT sColor of garrTheme
      	LooperAddLine(LOOP_Theme, HTMLToRGB(sColor), sColor)
      END
      // Deletes the initial selection
      LooperSelectMinus(LOOP_Theme)
