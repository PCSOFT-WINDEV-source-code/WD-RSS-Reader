         // Retrieves the rollover subscript
         nHoveredSubscript is int = LooperInfoXY(LOOP_Theme, liLineNumber, MouseXPos(), MouseYPos())
         
         // If we always hover the same line/the same control
         IF gnRolloverSubscript = nHoveredSubscript _AND_ gsHoveredControl = sHoveredControl THEN RETURN
         
         // Stores the new rollover subscripts
         gnRolloverSubscript = nHoveredSubscript
         gsHoveredControl = sHoveredControl
         
         // If no hovered control
         IF sHoveredControl <> "" THEN
         
