           type : 32
     -
       name : OPT_URL
       identifier : 0x286e647651d76258
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           internal_properties : CAAAAAgAAACxg1Tfs+NIBpEaVxiEDw5fGWvu3BZ3BzOhmJOeGLXpMSv+oACNKj0RUpmPhhQq45yp3FgEJgp1qXHgy/yxj3vXvra32qrHVzkbdeUTH2ldpaT+Z24D2okIy8uYMzDYWENC3kLI51BU9PrcDVY=
           code : |1+
            sURL is string
            StreamImage is an Image
            
            // Defines a default URL
            sURL = EDT_Address
            // Retrieves the clipboard content if it is a URL
            IF ClipboardFormat(cfText) _AND_ Clipboard [= "http"THEN
            	sURL = Clipboard()
            END
            
            // Opens the edit window
            IF Input(<ยง$0003ยง>, sURL, [<ยง$0004ยง>, <ยง$0005ยง>], 2, 2, dlgIconQuestion) = 2 THEN RETURN
            
            // Interrogates the URL
            MyQuery is a httpRequest
            MyResponse is a httpResponse
            
            // Stores the URL 
            MyQuery..URL = sURL
            MyQuery..IgnoreError = httpIgnoreRedirectToHTTPS
            // Interrogates the URL
            MyResponse = HTTPSend(MyQuery)	
            // If the query is OK (return code to 200), stores the image
            IF MyResponse..StatusCode = 200 _AND_ MyResponse..Content <> "" THEN
            	
            	// Saves the return in an image variable (to make sure that this return really is an image)
            	StreamImage = MyResponse..Content
            	IF StreamImage..Valid = False THEN RETURN
            	
            	// The return is an image, it must be stored
            	sFile is string = fTempFile()+".png"
            	dSaveImagePNG(StreamImage, sFile)
            	
            	// Displays the image
            	IMG_Image = sFile
            	// Indicates that the image is modified (to force its backup)
            	IMG_Image..Modified = True
            	
            END
           type : 32
