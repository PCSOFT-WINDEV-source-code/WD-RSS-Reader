        code : |1+
         // Stores the stream information
         ScreenToFile(WIN_Stream_Properties, RSSStream)
         
         // Modifies the image if necessary
         IF IMG_Image..Modified THEN
         	HLinkMemo(RSSStream, Image, IMG_Image, hMemoImg)
         	
         	// Updates the cache
         	
         	// Defines the path of the image
         	sFile is string = SET_Global.gsCacheDirectory +["\"]+ "stream" +["\"]+ RSSStream.RSSStreamID+".png"
         	// If this image exists, it must be deleted
         	IF fFileExist(sFile) THEN fDelete(sFile)
         	// Saves the image in the cache file
         	HExtractMemo(RSSStream, Image, sFile)
         END
         
         IF HModify(RSSStream) = False THEN
         	Error(<ยง$0001ยง>, HErrorInfo(hErrMessage))
         	RETURN
         END
         
         // Closes the window in validation
         Close("", True)
