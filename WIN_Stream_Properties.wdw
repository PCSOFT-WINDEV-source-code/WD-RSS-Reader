         -
           code : |1-
            sFile is string
            
            // Opens the file picker
            sFile = fSelect("", "", <ยง$0002ยง>, fGraphicFilter(), ExtractString(ExtractString(fGraphicFilter(), 2, TAB), 1, CR))
            IF sFile ~= "" THEN RETURN
            
            // Modifies the image
